SET NAMES utf8mb4;
SET FOREIGN_KEY_CHECKS = 0;

-- ----------------------------
-- Table structure for test
-- ----------------------------


#:table_#

DROP TABLE IF EXISTS [tableName];

CREATE TABLE [tableName]  (
  [columnList]
) ENGINE = InnoDB CHARACTER SET = utf8 COLLATE = utf8_general_ci ROW_FORMAT = Dynamic;

SET FOREIGN_KEY_CHECKS = 1;

#:table_$

#:column_#

`[columnName]` [columnType] CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL COMMENT '[comment]'

#:column_$


#:alter_#

ALTER TABLE [tableName] ADD COLUMN [columnName] [columnType] DEFAULT NULL COMMENT '[comment]';

#:alter_$


#:insert_#

INSERT INTO [tableName] ([columnList]) VALUES ([valueList]);

#:insert_$

#:select_#

SELECT * FROM [tableName] [WHERE] ORDER BY id DESC

#:select_$

#:schema_#
SELECT
	ALL_TAB_COLUMNS.OWNER AS table_schema,
	ALL_TAB_COLUMNS.TABLE_NAME,
	ALL_TAB_COLUMNS.COLUMN_NAME,
	ALL_TAB_COLUMNS.DATA_TYPE,
	ALL_TAB_COLUMNS.CHAR_LENGTH AS column_length,
	ALL_TAB_COLUMNS.NULLABLE,
	ALL_TAB_COLUMNS.DATA_PRECISION,
	ALL_TAB_COLUMNS.DATA_SCALE,
	ALL_TAB_COLUMNS.DATA_LENGTH,
	ALL_TAB_COLUMNS.CHAR_USED,
	ALL_TAB_COLUMNS.COLUMN_ID,
	ALL_COL_COMMENTS.COMMENTS AS column_comment,
	ALL_TAB_COLUMNS.DATA_DEFAULT,
	ALL_TAB_COLUMNS.DEFAULT_LENGTH,
	ALL_TAB_COLUMNS.NULLABLE AS is_nullable,
	ALL_TAB_COLUMNS.DATA_TYPE_MOD,
	ALL_TAB_COLUMNS.DATA_TYPE_OWNER
FROM
	ALL_TAB_COLUMNS,
	ALL_COL_COMMENTS
WHERE
	ALL_TAB_COLUMNS.OWNER = ALL_COL_COMMENTS.SCHEMA_NAME
	AND ALL_TAB_COLUMNS.TABLE_NAME = ALL_COL_COMMENTS.TABLE_NAME
	AND ALL_TAB_COLUMNS.COLUMN_NAME = ALL_COL_COMMENTS.COLUMN_NAME
	AND ALL_TAB_COLUMNS.OWNER NOT IN ( 'SYS', 'CTISYS' )
	AND NOT EXISTS ( SELECT * FROM ALL_VIEWS WHERE OWNER = ALL_TAB_COLUMNS.OWNER AND VIEW_NAME = ALL_TAB_COLUMNS.TABLE_NAME ) AND ALL_TAB_COLUMNS.TABLE_NAME = 'g_module'
#schema_$